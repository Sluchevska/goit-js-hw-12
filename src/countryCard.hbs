

{{#each this}}
    <h2>Name: {{name}}</h2>
    <img src = "{{flag}}" alt = "flag">
    <p>Capital: {{capital}}</p>
    <p>Population: {{population}}</p>
    
    <p>Language: {{languages}}</p>
    {{#each languages}}
    <ul>
        <li>{{languages.name}}</li>
    </ul>
    {{/each}}
    {{/each}}


